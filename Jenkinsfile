pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
      stage('Hi') {
        steps {
          echo 'Hi man'
        }
      }
      stage('Who') {
        steps {
          echo 'Who is the man'
        }
      }
      stage('Good') {
        steps {
          echo 'Good person'
        }
      }
    }
}
